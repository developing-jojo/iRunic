{
  "name": "irunic",
  "version": "0.0.1",
  "author": "iRunic Team",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "start": "ionic-app-scripts serve",
    "start:lab": "ionic-app-scripts serve --lab",
    "start:ios": "ionic-app-scripts build --target cordova --platform ios",
    "start:android": "ionic-app-scripts build --target cordova --platform android",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "build:apk": "cordova.cmd build android",
    "lint": "ionic-app-scripts lint"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "^5.31.1",
    "@ionic-native/geolocation": "^5.31.1",
    "@ionic-native/health": "^5.31.1",
    "@ionic-native/health-kit": "^5.31.1",
    "@ionic-native/splash-screen": "^5.31.1",
    "@ionic-native/status-bar": "^5.31.1",
    "@ionic/storage": "^2.3.1",
    "cordova-android": "9.0.0",
    "cordova-ios": "6.2.0",
    "gradle": "^1.2.3",
    "ionic-angular": "3.9.9",
    "ionicons": "3.0.0",
    "promise-polyfill": "^8.2.0",
    "rxjs": "^5.5.11",
    "rxjs-compat": "^6.6.3",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.4",
    "com.telerik.plugins.healthkit": "^0.7.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-health": "^2.0.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.2.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "needs": "^1.0.4",
    "typescript": "^4.1.3"
  },
  "description": "Another running app",
  "cordova": {
    "plugins": {
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      },
      "cordova-plugin-pedometer": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-health": {
        "HEALTH_READ_PERMISSION": "App needs read access",
        "HEALTH_WRITE_PERMISSION": "App needs write access"
      },
      "com.telerik.plugins.healthkit": {
        "HEALTH_READ_PERMISSION": "App needs read access",
        "HEALTH_WRITE_PERMISSION": "App needs write access"
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
